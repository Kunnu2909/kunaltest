{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": ["error", "single"],
    "eqeqeq": ["warn"],
    "no-empty-function": ["error"],
    "prefer-const": ["error"],
    "spaced-comment": ["warn"]
  }
}
